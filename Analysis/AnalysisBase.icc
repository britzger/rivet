// -*- C++ -*-
//
// This is the implementation of the inlined member functions of
// the AnalysisBase class.
//

namespace Rivet {

inline AnalysisBase::AnalysisBase()
  : theHandler(0) {}

inline AnalysisBase::AnalysisBase(const AnalysisBase & x)
  : theHandler(x.theHandler), info(x.info) {}

inline RivetHandler & AnalysisBase::handler() {
  return *theHandler;
}

}
