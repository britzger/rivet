// -*- C++ -*-
//
// This is the implementation of the inlined member functions of
// the Analysis class.
//

namespace Rivet {

  inline Analysis::Analysis()
    : theHandler(0) {}

  inline Analysis::Analysis(const Analysis & x)
    : theHandler(x.theHandler), info(x.info) {}

  inline RivetHandler & Analysis::handler() {
    return *theHandler;
  }

}
