// -*- C++ -*-
//
// This is the implementation of the inlined member functions of
// the Beam class.
//

namespace Rivet {

inline Beam::Beam() {}

inline Beam::Beam(const Beam & x)
  : Projection(x), theBeams(x.theBeams) {}

inline const ParticlePair & Beam::operator()() const {
  return theBeams;
}

}
