// -*- C++ -*-
//
// This is the implementation of the inlined member functions of
// the DISLepton class.
//

namespace Rivet {

inline DISLepton::DISLepton(Beam & beamp, long inid, long outid)
  : beams(&beamp), idin(inid), idout(outid) {
  info.declareParameter("BeamA", inid);
  info.declareParameter("DISLepton", outid, "none",
			"The type of the scattered lepton.");
}

inline DISLepton::DISLepton(const DISLepton & x)
  : Projection(x), beams(x.beams), idin(x.idin), idout(x.idout),
    incoming(x.incoming), outgoing(x.outgoing) {}

inline const Particle & DISLepton::in() const {
  return incoming;
}

inline const Particle & DISLepton::out() const {
  return outgoing;
}

}
