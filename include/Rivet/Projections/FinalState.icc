// -*- C++ -*-

namespace Rivet {

  inline FinalState::FinalState(double mineta, double maxeta, double minpt)
    : etamin(mineta), etamax(maxeta), ptmin(minpt) {}
  
  inline FinalState::FinalState(const FinalState & x)
    : Projection(x), etamin(x.etamin), etamax(x.etamax),
      ptmin(x.ptmin), theParticles(x.theParticles) {}
  
  inline const ParticleVector & FinalState::particles() const {
    return theParticles;
  }

}
