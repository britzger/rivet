// -*- C++ -*-

namespace Rivet {

  inline FinalStateProjection::FinalStateProjection(double mineta, double maxeta)
    : etamin(mineta), etamax(maxeta) {}
  
  inline FinalStateProjection::FinalStateProjection(const FinalStateProjection & x)
    : Projection(x), etamin(x.etamin), etamax(x.etamax),
      theParticles(x.theParticles) {}
  
  inline const ParticleVector & FinalStateProjection::particles() const {
    return theParticles;
  }

}
