// -*- C++ -*-
//
// This is the implementation of the inlined member functions of
// the Projection class.
//

namespace Rivet {

inline Projection::Projection() {}

inline Projection::Projection(const Projection &) {}

inline bool Projection::before(const Projection & p) const {
  const type_info & thisid = typeid(*this);
  const type_info & otherid = typeid(p);
  if ( thisid == otherid ) return compare(p) < 0;
  else return thisid.before(otherid);
}

}
