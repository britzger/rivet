// -*- C++ -*-
//
// This is the implementation of the inlined member functions of
// the CentralEtHCM class.
//

namespace Rivet {

inline CentralEtHCM::CentralEtHCM(long inid, long outid, long hadid,
				    double mineta, double maxeta)
  : fshcm(inid, outid, hadid, mineta, maxeta) {}

inline CentralEtHCM::
CentralEtHCM(const CentralEtHCM & x)
  : Projection(x), fshcm(x.fshcm), sumet(x.sumet) {}

inline double CentralEtHCM::sumEt() const {
  return sumet;
}

}
