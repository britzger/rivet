//
// This is the implementation of the inlined member functions of
// the FinalStateProjection class.
//

namespace Rivet {

inline FinalStateProjection::FinalStateProjection() {}

inline FinalStateProjection::
FinalStateProjection(const FinalStateProjection & x)
  : Projection(x), theParticles(x.theParticles) {}

inline const PVector & FinalStateProjection::particles() const {
  return theParticles;
}

}
